function combine(gr::GenomicRanges)
    out = similar(gr)
    out.strands[:] = STRAND_NA # move me to GenomicFeatures.similar
    x = sort(gr)
    current_end = 0
    i = 0
    for (s,e) in eachrange(x)
        if s > current_end
            i = i + 1
            _genostarts(out)[i] = s
        end
        if e > current_end
            _genoends(out)[i] = current_end = e
        end
    end
    resize!(out, i)
    out
end

