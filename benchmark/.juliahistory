function Base.convert(::Type{DataFrame}, x::GenomicPositions)
    chrs = chr_names(x)
    n = length(x)
    c_res = similar(chrs, n)
    p_res = similar(_genostarts(x), n)
    ends = chr_ends(x)
    offsets = chr_offsets(x)
    i = r = 1
    e = ends[r]
    o = offsets[r]
    c = chrs[r]
    @inbounds for g in _genostarts(x)
        if g > e || g <= o
            r = 1
            while g > ends[r]
                r = r + 1
            end
            e = ends[r]
            o = offsets[r]
            c = chrs[r]
        end
        c_res[i] = c
        p_res[i] = g - o
        i = i + 1
    end
    DataFrame( Chromosome = c_res, Position = p_res )
end

